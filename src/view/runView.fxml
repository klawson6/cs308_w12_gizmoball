<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>

<?import view.ResizableCanvas?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RunViewController" fx:id="rootPane">

    <ToolBar maxHeight="35">
            <Button text="Start" />
            <Button text="Stop" />
            <Button text="Tick" />
            <Button fx:id="buildButton" text="Build" />
            <Button fx:id="saveButton" text="Save" />
            <Button fx:id="loadButton" text="Load" />
            <Button fx:id="quitButton" text="Quit" />
    </ToolBar>
    <ResizableCanvas fx:id="canvas" width="700" height="665">
    </ResizableCanvas>
</VBox>
